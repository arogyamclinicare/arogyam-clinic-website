{
  "name": "arogyam-clinic",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --config vite.config.production.ts",
    "dev:https": "npm run generate-certs && vite --host --https --config vite.config.production.ts",
    "build": "tsc && vite build --config vite.config.production.ts",
    "build:prod": "set NODE_ENV=production && tsc && vite build --mode production --config vite.config.production.ts",
    "build:analyze": "npm run build:prod && npx vite-bundle-analyzer dist/stats.html",
    "preview": "vite preview --config vite.config.production.ts",
    "preview:prod": "vite preview --mode production --config vite.config.production.ts",
    "generate-certs": "node scripts/generate-https-certs.js",
    "production:check": "npm run lint:fix && npm run type-check && npm run security:scan && npm run build:prod",
    "production:deploy": "npm run production:check && npm run deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:security": "jest --selectProjects security",
    "test:performance": "jest --selectProjects performance",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:strict": "eslint . --ext ts,tsx --config .eslintrc.cjs --max-warnings 0",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --project tsconfig.strict.json",
    "clean": "rimraf dist node_modules/.vite certs",
    "clean:all": "rimraf dist node_modules/.vite node_modules package-lock.json certs",
    "reinstall": "npm run clean:all && npm install",
    "start": "npm run preview:prod",
    "deploy": "npm run build:prod && vercel --prod",
    "deploy:preview": "npm run build:prod && vercel",
    "analyze": "npm run build:analyze",
    "security:audit": "npm audit && npm audit fix",
    "security:scan": "npm run test:security && npm audit",
    "security:full": "npm run security:audit && npm run security:scan && npm run lint:strict",
    "performance:test": "npm run build:prod && lighthouse dist/index.html --output html --output-path ./lighthouse-report.html",
    "performance:monitor": "npm run test:performance",
    "optimize:images": "node scripts/optimize-images.cjs",
    "lighthouse": "lighthouse https://your-domain.vercel.app --output html --output-path ./lighthouse-report.html",
    "load:test": "node scripts/load-test.js",
    "load:test:local": "node scripts/load-test.js https://localhost:3000",
    "quality:check": "npm run type-check:strict && npm run lint:strict && npm run test:coverage",
    "quality:full": "npm run quality:check && npm run test:e2e && npm run security:full",
    "docs:generate": "typedoc --out docs src",
    "docs:serve": "http-server docs -p 8080",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "ai:feature": "node scripts/ai-development-workflow.js feature",
    "ai:bugfix": "node scripts/ai-development-workflow.js bugfix",
    "ai:refactor": "node scripts/ai-development-workflow.js refactor",
    "ai:quality": "node scripts/ai-development-workflow.js quality",
    "ai:workflow": "node scripts/ai-development-workflow.js",
    "cursor:optimize": "node scripts/cursor-pro-optimizer.js",
    "cursor:setup": "node scripts/setup-advanced-mcp.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@supabase/supabase-js": "^2.57.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "embla-carousel-react": "^8.1.3",
    "esbuild": "^0.25.9",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "input-otp": "^1.2.4",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.376.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.4",
    "react-intersection-observer": "^9.16.0",
    "react-resizable-panels": "^2.0.17",
    "recharts": "^2.12.7",
    "sharp": "^0.34.3",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^30.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "express": "^5.1.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "postcss": "^8.4.38",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.3",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^1.2.3"
  }
}
